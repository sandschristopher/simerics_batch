[ProgramPath]
	Simerics = C:/Program Files/Simerics/SimericsMP.exe

[PreProcessing]
	MeshGeometry = True
	runTransient = True
	rpm = Omega
	rpmData = relative
	rpmValues = 0.80 1.00 1.20
	flowRate = vflow_out
	flowRateData = relative
	flowRateValues = 0.60 0.80 1.00 1.20 1.40

[steady]
	BaseName = HT_single_stage_steady
	AveragingWindow = 50
	
[transient]
	BaseName = HT_single_stage_transient
	AveragingWindow = 120

[PostProcessing]
	FlowQuantities = DPtt Eff_tt PC
