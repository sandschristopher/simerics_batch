[ProgramPath]
	Simerics = C:/Program Files/Simerics/SimericsMP.exe

[PreProcessing]
	MeshGeometry = False
	runTransient = True
	batchVar1 = Omega2
	batchVar1Data = relative
	batchVar1Values = 0.80 1.00 1.20
	batchVar2 = vflow_out
	batchVar2Data = relative
	batchVar2Values = 1.00 1.20

[steady]
	BaseName = AP_nq149_steady
	AveragingWindow = 1
	
[transient]
	BaseName = AP_nq149_transient
	AveragingWindow = 120

[PostProcessing]
	FlowQuantities = DPtt Eff_tt PC2
